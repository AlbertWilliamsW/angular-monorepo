{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-a": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app-a",
      "sourceRoot": "projects/app-a/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-a",
            "baseHref": "/app-a/",
            "index": "projects/app-a/src/index.html",
            "browser": "projects/app-a/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-a/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-a/src/favicon.ico",
              "projects/app-a/src/assets"
            ],
            "styles": [
              "projects/app-a/src/styles.scss"
            ],
            "scripts": [],
            "server": "projects/app-a/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/app-a/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-a:build:production"
            },
            "development": {
              "buildTarget": "app-a:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app-a:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app-a/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-a/src/favicon.ico",
              "projects/app-a/src/assets"
            ],
            "styles": [
              "projects/app-a/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "app-b": {
